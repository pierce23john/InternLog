      "library": "font-awesome@6.2.0",
      "files": [ "css/all.css", "js/all.js" ],
      "destination": "wwwroot/lib/fontawesome/"
    },
    {
      "library": "materialize@1.0.0",
      "destination": "wwwroot/lib/materialize"
